# Kick API configuration
KICK__AuthDomain=https://id.kick.com
KICK__ApiBase=https://api.kick.com
KICK__ClientId= {CLIENT_ID}
KICK__ClientSecret= {CLIENT_SECRET}
KICK__RedirectUri=http://localhost:5173/callback
KICK__Scope=openid profile email user:read channel:read chat:write

# Chat listener
KICKCHAT__WebSocketUrl=wss://chat.kick.com

# Event webhook
KICKEVENT__CallbackUrl=http://localhost:5173/events/webhook
KICKEVENT__WebhookSecret= replace-with-secret
KICKEVENT__BotAccessToken= {BOT_ACCESS_TOKEN}
KICKEVENT__ChannelId=0

# App hosting (optional)
APP__ListenUrl=http://localhost:5173
